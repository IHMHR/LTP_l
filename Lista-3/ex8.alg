algoritmo "Lista3"
// Função : Fatorar
// Autor : IHMHR
// Data : 20/03/2015
// Seção de Declarações
var
cont,num,resul:inteiro
inicio
// Seção de Comandos
escreval("Digite o numero para ser fatorado:")
leia(num)

resul <- 1

para cont de 1 ate num faca
   resul <- resul * cont
fimpara

escreval(resul)
fimalgoritmo
