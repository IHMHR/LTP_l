algoritmo "ListaLTP"
// Função : area circuferencia
// Autor : IHMHR
// Data : 11/02/2015
// Seção de Declarações
var
   raio,area:real
inicio
// Seção de Comandos
   escreval("Digite o valor do raio da circuferência:")
   leia(raio)

   area <- 3.14 * raio^2

   escreval("O valor da área da circuferência é: ", area)
fimalgoritmo
