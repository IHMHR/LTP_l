algoritmo "ListaLTP"
// Função : calcular area retangulo
// Autor : IHMHR
// Data : 12/02/2015
// Seção de Declarações
var
   lado1,lado2,area:real
inicio
// Seção de Comandos
   escreval("Digite o valor do primeiro lado:")
   leia(lado1)
   escreval("Digite o valor do segundo lado:")
   leia(lado2)

   area <- lado1 * lado2

   escreval("O valor da área do retângulo é: ", area)
fimalgoritmo
