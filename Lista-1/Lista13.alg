algoritmo "ListaLTP"
// Função : arredondar
// Autor : IHMHR
// Data : 13/02/2015
// Seção de Declarações
var
   numero:real
inicio
// Seção de Comandos
   escreval("Digite um valor real:")
   leia(numero)
   
   numero <- Int(numero)
   
   escreval("O número arredondado é: ", numero)
fimalgoritmo
